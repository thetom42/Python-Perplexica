[MESSAGES CONTROL]
disable=C0111,C0103,C0301,C0325,C0411,C0412,C0413,W0611,W0612,W0613,W0621,W0703,R0903,R0913,R1705
ignore=CVS
ignore-patterns=
persistent=yes
load-plugins=
jobs=1
unsafe-load-any-extension=no
extension-pkg-whitelist=

[pylint.messages_control]
confidence=

[pylint.format]
max-line-length=120
max-module-lines=1000
indent-string='    '

[pylint.logging]
logging-modules=logging
